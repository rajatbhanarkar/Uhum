<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.somebody">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <supports-screens android:anyDensity="true"
        android:smallScreens="true"
        android:normalScreens="true"
        android:largeScreens="true"
        android:xlargeScreens="true"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".PhyListActivity"></activity>
        <activity android:name=".GifPlayerActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".WalkThroughActivity" />
        <activity android:name=".WebViewActivity" />
        <activity android:name=".DisplayProfileActivity" />
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".VideoPlayerActivity" />
        <activity android:name=".MedListActivity" />
        <activity android:name=".ProfileSetupActivity" />
        <activity android:name=".EnterNumberActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".PhysicalHealthActivity" />
        <activity android:name=".MentalHealthActivity" />
        <activity android:name=".HelpSomeoneActivity" />
        <activity android:name=".GetHelpActivity" />
        <activity android:name=".FAQActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".MusicPlayerActivity" />
        <activity android:name=".MainPageActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>